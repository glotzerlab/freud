{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>glotzerlab/renovate-config"
  ],
  "packageRules": [
    {
      "description": "Do not upgrade pinned python versions used for testing.",
      "matchManagers": ["pixi"],
      "matchDepTypes": ["feature-py*"],
      "matchPackageNames": ["python"],
      "enabled": false
    }
  ]
}
